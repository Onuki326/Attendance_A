<h1>出勤中社員一覧</h1>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <td>社員番号</td>
      <td>名前</td>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.employee_number %></td>
        <td><%= user.name%></td>
      </tr>
    <% end %>
  </tbody>
</table>