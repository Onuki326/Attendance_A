<% provide(:title, 'All users') %>
<h1>ユーザー一覧</h1>

<div class="row">
  <%= form_tag(admin_users_path, method: :post, multipart: true, class: "btn") do %>
    <%= file_field_tag :csv_file %>
    <%= submit_tag "CSVをインポート", class: "btn btn_default btn_key" %>
  <% end %>  
</div>

<%= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>